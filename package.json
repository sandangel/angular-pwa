{
  "name": "iot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --build-optimizer --app 0",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --single-quote --print-width 120 --write \"{apps,libs}/**/*.ts\"",
    "build:ssr": "yarn build:client-and-server-bundles && yarn webpack:server",
    "serve:ssr": "node dist/apps/admin-mobile/server.js",
    "build:client-and-server-bundles":
      "ng build --prod --build-optimizer && ng build --prod --build-optimizer --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.config.js --progress --colors",
    "ssr": "yarn build:ssr && yarn serve:ssr",
    "pwa": "sw-precache --root=dist/apps/admin-mobile/browser --config=precache-config.js"
  },
  "private": true,
  "dependencies": {
