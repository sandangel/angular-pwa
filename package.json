    "@nrwl/nx": "^0.1.0-beta.0",
    "@swimlane/ngx-charts": "^6.1.0",
    "angular-bootstrap-md": "^4.3.7",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
